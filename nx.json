{
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "affected": {
    "defaultBase": "origin/develop"
  },
  "npmScope": "krew",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api-client": {
      "tags": []
    },
    "krew-angular-feature-shell": {
      "tags": ["scope:angular", "type:feature"]
    },
    "krew-ui-angular": {
      "tags": [],
      "implicitDependencies": ["shared-assets"]
    },
    "krew-ui-react": {
      "tags": [],
      "implicitDependencies": ["shared-assets"]
    },
    "krew-ui-svelte": {
      "tags": [],
      "implicitDependencies": ["shared-assets"]
    },
    "krew-website": {
      "tags": []
    },
    "shared-assets": {
      "tags": ["scope:shared", "type:assets"]
    },
    "shared-environments": {
      "tags": ["scope:shared", "type:environments"]
    }
  }
}
