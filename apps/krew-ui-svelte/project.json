{
  "root": "apps/krew-ui-svelte",
  "sourceRoot": "apps/krew-ui-svelte/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nxext/svelte:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/krew-ui-svelte",
        "entryFile": "apps/krew-ui-svelte/src/main.ts",
        "tsConfig": "apps/krew-ui-svelte/tsconfig.app.json",
        "svelteConfig": "apps/krew-ui-svelte/svelte.config.cjs",
        "assets": [
          {
            "glob": "/*",
            "input": "apps/krew-ui-svelte/public/**",
            "output": "./"
          },
          {
            "glob": "**/*",
            "input": "libs/shared/assets/src/assets",
            "output": "assets"
          }
        ]
      },
      "configurations": {
        "production": {
          "dev": false,
          "fileReplacements": [
            {
              "replace": "libs/shared/environments/src/lib/environment.ts",
              "with": "libs/shared/environments/src/lib/environment.prod.ts"
            }
          ]
        },
        "development": {
          "dev": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nxext/svelte:build",
      "configurations": {
        "production": {
          "browserTarget": "krew-ui-svelte:build:production"
        },
        "development": {
          "browserTarget": "krew-ui-svelte:build:development"
        }
      },
      "options": {
        "port": 4403,
        "outputPath": "dist/apps/krew-ui-svelte",
        "entryFile": "apps/krew-ui-svelte/src/main.ts",
        "tsConfig": "apps/krew-ui-svelte/tsconfig.app.json",
        "svelteConfig": "apps/krew-ui-svelte/svelte.config.cjs",
        "assets": [
          {
            "glob": "/*",
            "input": "apps/krew-ui-svelte/public/**",
            "output": "./"
          }
        ],
        "watch": true,
        "serve": true
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/krew-ui-svelte/**/*.{ts,svelte,spec.ts}"]
      },
      "outputs": ["{options.outputFile}"]
    },
    "check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/krew-ui-svelte"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["shared-assets"]
}
