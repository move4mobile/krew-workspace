{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/krew-ui-svelte/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nxext/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/apps/krew-ui-svelte",
        "assets": [
          {
            "glob": "/*",
            "input": "./public/**",
            "output": "./"
          },
          {
            "glob": "**/*",
            "input": "libs/shared/assets/src/assets",
            "output": "assets"
          }
        ],
        "tsConfig": "apps/krew-ui-svelte/tsconfig.app.json"
      },
      "configurations": {
        "production": {
          "dev": false,
          "fileReplacements": [
            {
              "replace": "libs/shared/environments/src/lib/environment.ts",
              "with": "libs/shared/environments/src/lib/environment.prod.ts"
            }
          ]
        },
        "development": {
          "dev": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nxext/vite:dev",
      "configurations": {
        "production": {
          "browserTarget": "krew-ui-svelte:build:production"
        },
        "development": {
          "browserTarget": "krew-ui-svelte:build:development"
        }
      },
      "options": {
        "frameworkConfigFile": "@nxext/svelte/plugins/vite",
        "outputPath": "dist/apps/krew-ui-svelte",
        "tsConfig": "apps/krew-ui-svelte/tsconfig.app.json",
        "configFile": "apps/krew-ui-svelte/vite.config.ts"
      },
      "defaultConfiguration": "development"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/krew-ui-svelte/**/*.{ts,svelte,spec.ts}"
        ]
      }
    },
    "check": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "apps/krew-ui-svelte"
      }
    }
  },
  "implicitDependencies": [
    "shared-assets"
  ]
}